&НаСервере
Функция ПодготовитьДанныеДляЧекаНаСервере(кассир = "", описаниеОшибки = "", былаОшибка = ЛОЖЬ)
	былаОшибка = ЛОЖЬ;
	//Сообщить("ПодготовитьДанныеДляЧекаНаСервере");
	// Вставить содержимое обработчика.
	Если ТипЗнч(Объект.РеквизитДокумент) = Тип("ДокументСсылка.ОперацияПоПлатежнойКарте") или
		 ТипЗнч(Объект.РеквизитДокумент) = Тип("ДокументСсылка.ПриходныйКассовыйОрдер") или
		 ТипЗнч(Объект.РеквизитДокумент) = Тип("ДокументСсылка.РасходныйКассовыйОрдер") тогда
		//Сообщить("ОперацияПоПлатежнойКарте или ПКО или РКО");
		полнСтруктЧека = ПодготовитьМассивТоваровПоДокументуОплаты(Объект.РеквизитДокумент, кассир);
	иначеЕсли ТипЗнч(Объект.РеквизитДокумент) = Тип("ДокументСсылка.ЧекККМ") или 
				ТипЗнч(Объект.РеквизитДокумент) = Тип("ДокументСсылка.ЧекККМВозврат")
	тогда
		//Сообщить("ЧекККМ");
		полнСтруктЧека = ПодготовитьМассивТоваровПоДокументуОтгрузки(Объект.РеквизитДокумент, кассир);
	иначеЕсли ТипЗнч(Объект.РеквизитДокумент) = Тип("ДокументСсылка.РеализацияТоваровУслуг") тогда
		//Сообщить("РеализацияТоваровУслуг");	
		полнСтруктЧека = ПодготовитьМассивТоваровПоДокументуОтгрузки(Объект.РеквизитДокумент, кассир);
	иначеЕсли ТипЗнч(Объект.РеквизитДокумент) = Тип("ДокументСсылка.ВозвратТоваровОтКлиента") тогда
		//Сообщить("ВозвратТоваровОтКлиента");	
		полнСтруктЧека = ПодготовитьМассивТоваровПоДокументуОтгрузки(Объект.РеквизитДокумент, кассир);
	иначе
		описаниеОшибки = "Неизвестный тип документ " + СокрЛП(Тип(Объект.РеквизитДокумент));
		Сообщить(описаниеОшибки);
	конецЕсли;
	Возврат полнСтруктЧека;
КонецФункции //ПодготовитьДанныеДляЧекаНаСервере

&НаСервере
функция  ПодготовитьМассивТоваровПоДокументуОтгрузки(докумСсылка, кассир = "")
	резСтрук = Новый Структура;
	//Сообщить("ПодготовитьМассивТоваровПоДокументуОтгрузки нач");
	обдок = докумСсылка.ПолучитьОбъект();
	//шапка чека
	списПараметровЧека = Новый Структура;
	списПараметровЧека.Вставить("СНО", "patent");
	//списПараметровЧека.Вставить("Наличными", 1);
	ЭтоЧекВозрата = ЛОЖЬ;
	типРасчета = "полный расчет";
	//Сообщить("ПодготовитьМассивТоваровПоДокументуОтгрузки нач1");
	Если (ТипЗнч(докумСсылка) = Тип("ДокументСсылка.ЧекККМ")) или (ТипЗнч(докумСсылка) = Тип("ДокументСсылка.ЧекККМВозврат")) тогда
		СуммаОплатыНаличными = обдок.ПолученоНаличными;
		СуммаОплатыБезналом  = обдок.ОплатаПлатежнымиКартами.Итог("Сумма");
		СуммаОплатыСертификатом =  обдок.ПодарочныеСертификаты.Итог("Сумма");
		Если СуммаОплатыНаличными > 0 тогда
			списПараметровЧека.Вставить("Наличными", СуммаОплатыНаличными);
		конецЕсли;
		Если СуммаОплатыБезналом > 0 тогда
			списПараметровЧека.Вставить("Безналичными", СуммаОплатыБезналом);
		конецЕсли;		
		Если СуммаОплатыСертификатом > 0 тогда
			списПараметровЧека.Вставить("ЗачетАванса", СуммаОплатыСертификатом);
		конецЕсли;				
	иначеЕсли ТипЗнч(докумСсылка) = Тип("ДокументСсылка.РеализацияТоваровУслуг") или 
			ТипЗнч(докумСсылка) = Тип("ДокументСсылка.ВозвратТоваровОтКлиента") тогда
		списПараметровЧека.Вставить("Кредит", обдок.СуммаДокумента);
		типРасчета = "рассрочка"; //или "частичный расчет"
	конецЕсли;	
	//Сообщить("ПодготовитьМассивТоваровПоДокументуОтгрузки нач2");
	Если (ТипЗнч(докумСсылка) = Тип("ДокументСсылка.ВозвратТоваровОтКлиента")) или 
		 (ТипЗнч(докумСсылка) = Тип("ДокументСсылка.ЧекККМВозврат")) тогда
		ЭтоЧекВозрата = ИСТИНА;
	конецЕсли;
	//Сообщить("ПодготовитьМассивТоваровПоДокументуОтгрузки нач3");
	списПараметровЧека.Вставить("Кассир", ПолучитьИмяКассира(кассир));
	списПараметровЧека.Вставить("ЭтоЧекВозврата", ЭтоЧекВозрата);
	//Сообщить("ПодготовитьМассивТоваровПоДокументуОтгрузки нач4");
	//позиции
	массивЧека = Новый Массив;  
	//Сообщить("ПодготовитьМассивТоваровПоДокументуОтгрузки нач5");
	массивЧека = ПодготовитьМассивТоваровПоТабличнойЧастиДокументаОтгрузки(массивЧека, докумСсылка, типРасчета);
	//Сообщить("ПодготовитьМассивТоваровПоДокументуОтгрузки нач6");
			
	резСтрук.Вставить("списПараметровЧека", списПараметровЧека);
	резСтрук.Вставить("массивЧека", массивЧека);
	//Сообщить("ПодготовитьМассивТоваровПоДокументуОтгрузки кон");
	Возврат резСтрук;	
конецФункции //ПодготовитьМассивТоваровПоДокументуОтгрузки

&НаСервере
функция ПодготовитьМассивТоваровПоДокументуОплаты(докумСсылка, кассир = "")
	резСтрук = Новый Структура;
	об = докумСсылка.ПолучитьОбъект();
	//шапка чека
	списПараметровЧека = Новый Структура;
	списПараметровЧека.Вставить("СНО", "patent");
	//списПараметровЧека.Вставить("Наличными", 1);
	ЭтоЧекВозрата = ЛОЖЬ;
	Если ТипЗнч(докумСсылка) = Тип("ДокументСсылка.ОперацияПоПлатежнойКарте") тогда
		списПараметровЧека.Вставить("Безналичными", об.СуммаДокумента);
		Если об.ХозяйственнаяОперация <> Перечисления.ХозяйственныеОперации.ПоступлениеОплатыОтКлиента 
		тогда
			 ЭтоЧекВозрата = ИСТИНА;
		конецЕсли;		
	иначеЕсли ТипЗнч(докумСсылка) = Тип("ДокументСсылка.ПриходныйКассовыйОрдер") или 
			ТипЗнч(докумСсылка) = Тип("ДокументСсылка.РасходныйКассовыйОрдер") тогда
		списПараметровЧека.Вставить("Наличными", об.СуммаДокумента);
	конецЕсли;
	
	//списПараметровЧека.Вставить("ЗачетАванса", 0);
	списПараметровЧека.Вставить("Кассир", ПолучитьИмяКассира(кассир));
	списПараметровЧека.Вставить("ЭтоЧекВозврата", ЭтоЧекВозрата);
	//позиции
	массивЧека = Новый Массив;                  
	//массивЧека.Добавить(Новый Структура);
	//списПараметровПозиции = массивЧека[0];	
	
	списОснованийПлатежа = Новый СписокЗначений;
	типРасчета = "аванс";
	Для Каждого ТекСтрока Из об.РасшифровкаПлатежа Цикл 
		текОснованиеПлатежа = "";
		Если ТипЗнч(докумСсылка) <> Тип("ДокументСсылка.РасходныйКассовыйОрдер") тогда
			текОснованиеПлатежа = ТекСтрока.ОснованиеПлатежа;
		конецЕсли;
		если СокрЛП(текОснованиеПлатежа) = "" тогда
			текОснованиеПлатежа = ТекСтрока.Заказ;
		конецЕсли;
		Если СокрЛП(текОснованиеПлатежа) = "" тогда
			ПродолжитЬ;
		конецЕсли;		
		Если ТипЗнч(текОснованиеПлатежа) = Тип("ДокументСсылка.РеализацияТоваровУслуг") или 
				ТипЗнч(текОснованиеПлатежа) = Тип("ДокументСсылка.ВозвратТоваровОтКлиента") Тогда				
			//полный расчет или оплата кредита
			типРасчета = "оплата рассрочки";
			Если НачалоДня(ТекСтрока.ОснованиеПлатежа.ДатаДок) = НачалоДня(об.ДатаДок) тогда
				типРасчета = "полный расчет";
			конецЕсли;
			Если НЕ ЧекРеализацииИлиВозвратаБылПробит(текОснованиеПлатежа) тогда
				типРасчета = "полный расчет";
			конецЕсли;
		конецЕсли;
		Если ТипЗнч(текОснованиеПлатежа) = Тип("ДокументСсылка.ЗаказКлиента") Тогда
			//предоплата
			типРасчета = "предоплата 100%";
			если текОснованиеПлатежа.СуммаДокумента > ТекСтрока.Сумма тогда
				типРасчета = "предоплата частичная";
			конецЕсли;
		КонецЕсли;				
		списОснованийПлатежа.Добавить(текОснованиеПлатежа, типРасчета);
	КонецЦикла;		
	
	// Подготовка таблицы товаров
	всегоОснов = списОснованийПлатежа.Количество();
	Сообщить("всегоОснов="+СокрЛП(всегоОснов));
	если всегоОснов > 0 тогда		
		для счОсн = 0 по всегоОснов-1 цикл
			текЭлСписка = списОснованийПлатежа.Получить(счОсн);
			основ = текЭлСписка.Значение;
			основОб = основ.ПолучитьОбъект();
			типРасчета = СокрЛП(текЭлСписка.Представление);
			Сообщить("типРасчета2="+СокрЛП(типРасчета));
			
			массивЧека = ПодготовитьМассивТоваровПоТабличнойЧастиДокументаОтгрузки(массивЧека, основ, типРасчета);			
		конецЦикла;
	иначе
		массивЧека.Добавить(Новый Структура);
		списПараметровПозиции = массивЧека[массивЧека.Количество()-1];							
		
		ПринятоОт = ОбщегоНазначенияУТВызовСервера.ЗначениеРеквизитаОбъекта(об.Контрагент, "НаименованиеПолное");
		СтрТовара = НСтр("ru = 'Оплата от:'") + " " + ПринятоОт + Символы.ПС
		+ НСтр("ru = 'Основание:'") + " ";		
		списПараметровПозиции.Вставить("Товар", СокрЛП(СтрТовара));
		списПараметровПозиции.Вставить("Цена", об.СуммаДокумента);
		списПараметровПозиции.Вставить("Количество", 1);
		списПараметровПозиции.Вставить("ПредметРасчета", "Платеж");
		списПараметровПозиции.Вставить("ПризнакРасчета", "аванс");
		списПараметровПозиции.Вставить("НомерСекции", ПолучитьНомерСекцииДляАванса());
	конецЕсли;	
	
	резСтрук.Вставить("списПараметровЧека", списПараметровЧека);
	резСтрук.Вставить("массивЧека", массивЧека);
	Возврат резСтрук;
конецФункции //ПодготовитьМассивТоваровПоДокументуОплаты

&НаСервере
функция ПодготовитьМассивТоваровПоТабличнойЧастиДокументаОтгрузки(массивЧека, докумСсылка, типРасчета = "")
	Перем докумРеалиЧекаОьъект;
	докумРеалиЧекаОьъект = докумСсылка.ПолучитьОбъект();
	Если типРасчета = "" тогда
		типРасчета = "полный расчет";
		Если ТипЗнч(докумСсылка) <> Тип("ДокументСсылка.ЧекККМ") тогда
			типРасчета = "рассрочка";
		конецЕсли;
	конецЕсли;
	Для каждого стрТов Из докумРеалиЧекаОьъект.Товары Цикл				
	    НомерСекции = 2;
		Если НЕ НомеклатураЯвляетсяУслугой(стрТов.Номенклатура) тогда
			НомерСекции = 1;
		конецЕсли;
		добПозицияТовара = стрТов.Номенклатура;
		естьРодительЛок = ИСТИНА;
		добПозицияТовара = ПолучитьРодителяТовара(стрТов.Номенклатура, естьРодительЛок);
		если НЕ естьРодительЛок тогда
			добПозицияТовара = стрТов.Номенклатура;
		конецЕсли;
		массивЧека.Добавить(Новый Структура);
		списПараметровПозиции = массивЧека[массивЧека.Количество()-1];					
		списПараметровПозиции.Вставить("Товар", СокрЛП(добПозицияТовара));
		ценаТовара = ПолучитьЦенуТовараСервер(стрТов.Сумма, стрТов.Количество);
		списПараметровПозиции.Вставить("Цена", ценаТовара);
		списПараметровПозиции.Вставить("Количество", стрТов.Количество);
		списПараметровПозиции.Вставить("ПредметРасчета", "Товар");
		списПараметровПозиции.Вставить("ПризнакРасчета", типРасчета);
		Если ОпределитьЭтоПлатеж(типРасчета) тогда
			списПараметровПозиции.Вставить("ПредметРасчета", "Платеж");
			списПараметровПозиции.Вставить("Количество", 1);
		конецЕсли;
		списПараметровПозиции.Вставить("НомерСекции", НомерСекции);				
	КонецЦикла; 
	возврат массивЧека;
конецФункции //ПодготовитьМассивТоваровПоТабличнойЧастиДокументаОтгрузки

&НаСервере
функция ПолучитьЦенуТовараСервер(сумма, количество)
	ценаРез = сумма;
	Если количество <> 0 тогда
		ценаРез = Окр(сумма/количество, 2);
	конецЕсли;
	возврат ценаРез;
конецФункции //ПолучитьЦенуТовараСервер

&НаСервере
