&НаКлиенте
Процедура КомандаXОтчетНаККТ(Команда, НеЗакрыватьСоединениеСККТ = ЛОЖЬ)
	структураНастройкаиСвязиСКТТ = ПрочитатьНастройкиККТКлиент();
	имяКассира = ПолучитьЗначениеНастройки(структураНастройкаиСвязиСКТТ, "ИмяКассира");
	Если СокрЛП(РеквизитКассир) <> "" тогда
		имяКассира = РеквизитКассир;
	конецЕсли;
	УровеньОтладки = ПолучитьЗначениеНастройки(структураНастройкаиСвязиСКТТ, "УровеньОтладки");
	имяКассира = ПолучитьИмяКассира(имяКассира);	
	исходящийJSON = СформироватьJSONXОтчета(имяКассира, УровеньОтладки);
	ответJSON = "";
	описаниеОшибки = ""; КодОшибки = 0;
	резКомандыXОтчета = ВыполнитьЗаданиеJSON(COMДрайверККТ10, структураНастройкаиСвязиСКТТ, исходящийJSON, 
					   		   		 ответJSON, описаниеОшибки, КодОшибки,
										?(НеЗакрыватьСоединениеСККТ, ЛОЖЬ, ИСТИНА), НеЗакрыватьСоединениеСККТ);
	РезультатXОчтета = Новый Структура;
	РезультатXОчтета.Вставить("Результат", резКомандыXОтчета);									 
	РезультатXОчтета.Вставить("JSONЗапрос", исходящийJSON);
	РезультатXОчтета.Вставить("ответJSON", ответJSON);													 										 									 
	если не резКомандыXОтчета тогда 
		стрСообщиенияОшибки = "смена не открыта по причине: " + описаниеОшибки + "("+СокрЛП(КодОшибки) + ")";
		РезультатXОчтета.Вставить("описаниеОшибки", стрСообщиенияОшибки);
		РезультатXОчтета.Вставить("КодОшибки", КодОшибки);
		Элементы.НадписьРезудьтатПечатиЧека.Заголовок = стрСообщиенияОшибки;
	иначе
		стрСообщиенияИфно = "X отчет сформиррован";
		Элементы.НадписьРезудьтатПечатиЧека.Заголовок = стрСообщиенияИфно;
		Сообщить(стрСообщиенияИфно);		
	конецЕсли;     										
	Если ВРЕГ(Команда) = ВРЕГ("Закрыть") тогда
		ПодключитьОбработчикОжидания("ЖдемЧутьЧутьЧтобыФормаЗакрылась", 0.1, Истина);
		Закрыть(РезультатXОчтета);
	конецЕсли;	
КонецПроцедуры //КомандаОткрытьСменуНаККТ

&НаКлиенте
Процедура КомандаОткрытьСменуНаККТ(Команда, НеЗакрыватьСоединениеСККТ = ЛОЖЬ)
	структураНастройкаиСвязиСКТТ = ПрочитатьНастройкиККТКлиент();
	имяКассира = ПолучитьЗначениеНастройки(структураНастройкаиСвязиСКТТ, "ИмяКассира");
	Если СокрЛП(РеквизитКассир) <> "" тогда
		имяКассира = РеквизитКассир;
	конецЕсли;	
	УровеньОтладки = ПолучитьЗначениеНастройки(структураНастройкаиСвязиСКТТ, "УровеньОтладки");
	имяКассира = ПолучитьИмяКассира(имяКассира);	
	исходящийJSON = СформироватьJSONОткрытияСмены(имяКассира, УровеньОтладки);
	ответJSON = "";
	описаниеОшибки = ""; КодОшибки = 0;
	резКомандыОткрытияСмены = ВыполнитьЗаданиеJSON(COMДрайверККТ10, структураНастройкаиСвязиСКТТ, исходящийJSON, 
					   		   		 ответJSON, описаниеОшибки, КодОшибки,
										?(НеЗакрыватьСоединениеСККТ, ЛОЖЬ, ИСТИНА), НеЗакрыватьСоединениеСККТ);
	РезультатОткрытияСмены = Новый Структура;
	РезультатОткрытияСмены.Вставить("Результат", резКомандыОткрытияСмены);									 
	РезультатОткрытияСмены.Вставить("JSONЗапрос", исходящийJSON);
	РезультатОткрытияСмены.Вставить("ответJSON", ответJSON);													 										 									 
	если не резКомандыОткрытияСмены тогда 
		стрСообщиенияОшибки = "смена не открыта по причине: " + описаниеОшибки + "("+СокрЛП(КодОшибки) + ")";
		РезультатОткрытияСмены.Вставить("описаниеОшибки", стрСообщиенияОшибки);
		РезультатОткрытияСмены.Вставить("КодОшибки", КодОшибки);
		Элементы.НадписьРезудьтатПечатиЧека.Заголовок = стрСообщиенияОшибки;
	иначе
		номерСмены = ПолучитьЗнчениеПоляИзJSON(ответJSON, "shiftNumber");                    
		РезультатОткрытияСмены.Вставить("Номер_смены", номерСмены);									 
		стрСообщиенияИфно = "смена №" + СокрЛП(РезультатОткрытияСмены.Номер_смены) + " открыта";
		Элементы.НадписьРезудьтатПечатиЧека.Заголовок = "смена №" + СокрЛП(РезультатОткрытияСмены.Номер_смены) + " закрыта";
		Сообщить(стрСообщиенияИфно);		
	конецЕсли;     										
	Если ВРЕГ(Команда) = ВРЕГ("Закрыть") тогда
		ПодключитьОбработчикОжидания("ЖдемЧутьЧутьЧтобыФормаЗакрылась", 0.1, Истина);
		Закрыть(РезультатОткрытияСмены);
	конецЕсли;	
КонецПроцедуры //КомандаОткрытьСменуНаККТ
		
